{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "include": [
        "src/*"
      ],
      "exclude": [
        "node_modules",
        "dist"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}